<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="employee" class="com.demo.model.Employee">
		<property name="employeeId" value="4325353"></property>
		<property name="employeeName" value="Allen"></property>
	</bean>

	<bean id="pancard1" class="com.demo.model.Pancard">
		<property name="panHolderName" value="Allen"></property>
		<property name="panNo" value="QWERT5678L"></property>
	</bean>
	
	<bean id="pancard2" class="com.demo.model.Pancard">
		<property name="panHolderName" value="Allen"></property>
		<property name="panNo" value="ABCDR4567E"></property>
	</bean>
</beans>